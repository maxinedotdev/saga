{
  "name": "@maxinedotdev/mcp-documentation-server",
  "version": "1.12.0",
  "description": "A TypeScript MCP server for document management and semantic search with embeddings, featuring web crawling, multiple AI providers, and intelligent chunking",
  "main": "dist/server.js",
  "type": "module",
  "bin": {
    "mcp-documentation-server": "dist/server.js",
    "mcp-documentation-embeddings": "dist/embedding-cli.js"
  },
  "homepage": "https://github.com/maxinedotdev/mcp-documentation-server#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxinedotdev/mcp-documentation-server.git"
  },
  "bugs": {
    "url": "https://github.com/maxinedotdev/mcp-documentation-server/issues"
  },
  "scripts": {
    "build": "tsc && shx chmod +x dist/*.js",
    "start": "tsx src/server.ts",
    "dev": "fastmcp dev src/server.ts",
    "dev:direct": "npx tsx src/server.ts",
    "dev:build": "npm run build && node dist/server.js",
    "embeddings:check": "node dist/embedding-cli.js",
    "inspect": "npx fastmcp inspect src/server.ts",
    "test": "echo \"Warning: no test specified\" && exit 0",
    "test:unit": "tsx src/vector-db/__tests__/vector-db.test.ts",
    "test:integration": "tsx src/__tests__/integration.test.ts",
    "test:benchmark": "tsx src/__tests__/benchmark.test.ts",
    "test:all": "npm run test:unit && npm run test:integration",
    "prepare": "npm run build"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "documentation",
    "search",
    "embeddings"
  ],
  "author": "maxinedotdev",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@tsconfig/node22": "^22.0.1",
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
    "@types/html-to-text": "^9.0.4",
    "shx": "^0.3.4",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@google/genai": "^1.19.0",
    "@lancedb/lancedb": "^0.23.0",
    "@modelcontextprotocol/sdk": "^1.12.3",
    "@types/node": "^24.0.1",
    "@xenova/transformers": "^2.17.2",
    "dotenv": "^16.5.0",
    "fastmcp": "^3.24.0",
    "fs-extra": "^11.3.0",
    "glob": "^11.0.3",
    "html-to-text": "^9.0.5",
    "unpdf": "^1.2.1",
    "zod": "^3.25.64"
  },
  "publishConfig": {
    "access": "public"
  }
}
